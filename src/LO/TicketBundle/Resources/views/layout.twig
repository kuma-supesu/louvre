<!DOCTYPE html>
<html>
    {% block head %}
        <head>
        <meta charset="UTF-8">
       <title>{% block title %}Mus√©e du Louvre{% endblock %}</title>
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
        <link rel="icon" href="https://www.louvre.fr/sites/default/files/favicon.ico" type="image/x-icon"/>
        <link rel="shortcut icon" href="https://www.louvre.fr/sites/default/files/favicon.ico" type="image/x-icon"/>
        <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,700" rel="stylesheet"/>
        <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,400,300,500,600,700" rel="stylesheet"/>
        <link rel="stylesheet" type="text/css" href="//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"/>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous"/>
        <link rel="stylesheet" href="{{ asset('css/general.css') }}"/>
        <link rel="stylesheet" href="{{ asset('css/form.css') }}"/>
        <link rel="stylesheet" href="{{ asset('css/main.css') }}"/>
    </head>
    {% endblock %}

        {% block body %}
            <body>
            {% block header %}
                <header id="header">
                        <div class="container header-top">
                            <div class="row">
                                <div class="col-6 top-head-left"></div>
                                <div class="col-6 top-head-right">
                                    <ul>
                                        <li><a href="https://fr-fr.facebook.com/museedulouvre/" alt="Facebook"><i class="fa fa-facebook"></i></a></li>
                                        <li><a href="https://twitter.com/museelouvre"><i class="fa fa-twitter"></i></a></li>
                                        <li><a href="https://www.pinterest.fr/museedulouvre/"><i class="fa fa-pinterest"></i></a></li>
                                        <li><a href="https://www.youtube.com/user/louvre"><i class="fa fa-youtube"></i></a></li>
                                        <li><a href="https://www.instagram.com/museelouvre/"><i class="fa fa-instagram"></i></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    <hr/>
                    <div class="container">
                        <div class="row align-items-center justify-content-between d-flex">
                            <div id="logo">
                                <a href="{{ path('lo_commande_homepage') }}"><img src="{{asset ('img/logo_banner.png')}}" alt="Logo du Louvre" title="Home" /></a>
                            </div>
                            <nav id="nav-menu-container">
                                <ul class="nav-menu">
                                    <li class="menu-active"><a href="{{ path('lo_commande_homepage') }}">Home</a></li>
                                    <li><a href="{{ path('lo_commande_form') }}">Ticket</a></li>
                                    <li><a href="{{ path('lo_commande_renvoi') }}">Renvoi des Tickets</a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>
            </header>
            {% endblock %}

            {% block banner %}
                <section class="banner-area relative">
                    <div class="overlay overlay-bg"></div>
                    <div class="container">
                        <div class="row d-flex align-items-center justify-content-center">
                            <div class="about-content col-lg-12">
                                <h1 class="text-white">
                                    {{ title_page }}
                                </h1>
                            </div>
                        </div>
                    </div>
                </section>
            {% endblock %}

        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
            <script src="{{ asset('js/superfish.min.js') }}"></script>
            <script src="{{ asset('js/jquery.magnific-popup.min.js') }}"></script>
            <script src="{{ asset('js/main.js') }}"></script>
        {% endblock %}
            </body>
        {% endblock %}
</html>